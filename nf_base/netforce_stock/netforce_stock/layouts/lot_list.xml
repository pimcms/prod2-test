<list model="stock.lot">
    <top>
        <button string="Update Lot Expiry Transactions" method="update_expired_lots" show_progress="1"/>
    </top>
    <head>
        <button string="Print" icon="print" action="report_lot_form" action_options='{"ids":ids}'/>
    </head>
    <field name="number"/>
    <field name="received_date"/>
    <field name="mfg_date"/>
    <field name="expiry_date"/>
    <field name="life_remain_percent"/>
    <field name="weight"/>
    <field name="description"/>
    <field name="product_id"/>
</list>

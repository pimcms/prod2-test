<form model="report.stock.invoice">
    <group form_layout="stacked">
        <field name="date_from" span="2"/>
        <field name="date_to" span="2"/>
        <field name="product_id" span="2"/>
        <field name="categ_id" span="2"/>
        <field name="pick_id" span="2"/>
        <newline/>
        <field name="products" span="12" view="field_multi_select"/>
    </group>
</form>

<list model="purchase.order" priority="1">
    <top>
        <button string="Purchase Wizard" icon="plus" action="purchase_wizard"/>
    </top>
    <head>
        <!--<button string="Print" icon="print" action="report_purchase_form" action_options='{"convert":"pdf"}'/>
        <button string="Print (New)" icon="print" action="report_purchase_form2" action_options='{"ids":ids}'/>-->
        <button string="Print" icon="print" action="report_purchase_form3" action_options='{"ids":ids}'/>
        <button string="Merge Orders" method="merge_orders" confirm="Are you sure?"/>
        <button string="To Draft" method="to_draft" confirm="Are you sure?"/>
        <button string="Void" method="void" confirm="Are you sure?"/>
    </head>
    <field name="number"/>
    <field name="ref"/>
    <field name="contact_id"/>
    <field name="date"/>
    <field name="is_delivered"/>
    <field name="is_paid"/>
    <field name="state"/>
    <field name="amount_total"/>
    <field name="currency_id"/>
    <field name="procurement_employee_id"/>
</list>

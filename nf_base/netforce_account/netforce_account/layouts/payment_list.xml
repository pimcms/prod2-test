<list model="account.payment">
    <head>
        <button string="Print" icon="print" action="report_payment_form3" action_options='{"ids":ids}'/>
        <button string="Post" method="post" show_progress="1"/>
        <button string="Void" method="void"/>
        <button string="To Draft" method="to_draft"/>
        <button string="Update Stored" method="function_store"/>
    </head>
    <field name="number"/>
    <field name="date"/>
    <field name="contact_id"/>
    <field name="type"/>
    <field name="pay_type"/>
    <field name="account_id"/>
    <field name="ref"/>
    <field name="amount_total"/>
    <field name="state"/>
    <field name="user_id"/>
    <field name="memo"/>
</list>

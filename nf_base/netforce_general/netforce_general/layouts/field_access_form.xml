<form model="field.access">
    <top>
        <button string="Options">
            <item string="Duplicate Field Access" action="field_access_add" action_context='{"refer_id":id}'/>
        </button>
    </top>

    <field name="model_id"/>
    <field name="field_id" condition='[["model_id","=",model_id]]'/>
    <field name="profile_id"/>
    <field name="user_id"/>
    <field name="access"/>
</form>
